<div class="box box-primary">
  <div class="box-body">
    <div class="row">
      <div class="col-md-2">
        <%= link_to 'Nuevo Objeto', new_objeto_path(@objeto), {:class=>"btn btn-primary", style:"color:white"}%>
      </div>
    </div>
    <br>
    <table class="table table-bordered table-striped table-responsive">
      <thead>
        <tr>
          <th>Proceso</th>
          <th>Tipo proceso</th>
          <th>Nombre</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @objetos.each do |objeto| %>
          <tr>
            <td><%= objeto.proceso %></td>
            <td><%= objeto.tipo_proceso %></td>
            <td><%= objeto.nombre %></td>
            <td><%= link_to '', edit_objeto_path(objeto), class: 'btn btn-default btn-sm glyphicon glyphicon-pencil', title: 'Editar', style: 'color: black;' %>
            <%= link_to '', objeto, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-default btn-sm glyphicon glyphicon-trash', style:"color:black", title: 'Eliminar' %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
